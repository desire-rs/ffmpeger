  },
  "devDependencies": {
    "@parcel/transformer-pug": "2.7.0",
    "@parcel/transformer-vue": "2.7.0",
    "parcel": "^2.7.0"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "lodash": "^4.17.21",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "targets": {
    "main": false
  }
}
