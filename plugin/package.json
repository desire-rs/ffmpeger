{
  "name": "ffmpeger-plugin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "parcel serve src/index.pug",
    "dev2": "ENV_NAME=dev tsc src/scripts/*.ts --outDir dist/",
